{
  "name": "next-app",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "commit": "cz",
    "dev": "next dev",
    "lint:cspell": "cspell lint --no-progress --relative --no-must-find-files --dot --gitignore \"**\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts,tsx}\"",
    "lint:eslint:fix": "eslint --fix \"**/*.{js,mjs,cjs,ts,tsx}\"",
    "lint:markdownlint": "markdownlint --dot \"**/*.md\"",
    "lint:markdownlint:fix": "markdownlint --dot --fix \"**/*.md\"",
    "lint:prettier": "prettier --check --ignore-unknown \"**\"",
    "lint:prettier:fix": "prettier --write --ignore-unknown \"**\"",
    "lint:sort-package-json": "sort-package-json --check",
    "lint:sort-package-json:fix": "sort-package-json",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "lint:stylelint:fix": "stylelint --fix \"**/*.css\"",
    "lint:tsc": "tsc --noEmit",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky",
    "start": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "lodash": "^4.17.21",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@next/eslint-plugin-next": "^14.1.0",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "cspell": "^8.3.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-vitest": "^0.3.22",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "markdownlint-cli": "^0.39.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sort-package-json": "^2.7.0",
    "stylelint": "^16.2.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "vitest": "^1.2.2"
  }
}
